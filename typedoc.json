{
  "entryPoints": ["src/app"], // Points to your app directory with all components
  "out": "docs", // Where the documentation will be generated
  "entryPointStrategy": "expand", // Expand all modules
  "exclude": ["**/*.spec.ts"], // Exclude unit test files
  "excludePrivate": true, // Exclude private methods and properties
  "excludeProtected": false, // Includes protected methods and properties
  "name": "My Angular App", // Name of your Angular app
  "theme": "default" // Default theme for documentation
}
